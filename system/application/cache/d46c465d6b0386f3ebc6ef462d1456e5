    function size() {
      var height = Math.max($('#container').height(), $('#sideLeft').height(), $('#sideRight').height());
      $('#container').height(height);
      $('#sideLeft').height(height);
      $('#sideRight').height(height);
      $('#wrapper').css('background-position', 'right ' + ($('#wrapper').height() - 70 - 170) + 'px');
    }
  </script>
